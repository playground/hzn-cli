{
  "name": "hzn-cli",
  "version": "0.1.3",
  "description": "Open Horizon CLI toolkit helps streamline the process of preparing node agents and perform tasks between orgs environments",
  "main": "./build/index.js",
  "bin": {
    "oh": "./build/index.js"
  },
  "scripts": {
    "start": "nodemon -e ts --watch 'src/**/*.ts' --exec 'npm run build'",
    "build": "npx tsc && chmod +x build/*.js && chmod +x build/**/*.js",
    "local": "npm run build && sudo npm i -g && oh",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "keywords": [
    "IEAM",
    "Open",
    "Horizon",
    "edge",
    "computing",
    "MMS",
    "Model",
    "Management",
    "Service"
  ],
  "author": "ljeff@us.ibm.com",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.9.1",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/parser": "^4.30.0",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^8.2.0",
    "dotenv": "^10.0.0",
    "figlet": "^1.5.2",
    "jsonfile": "^6.1.0",
    "prompt": "^1.2.0",
    "rxjs": "^7.3.0",
    "yargs": "^17.1.1"
  }
}
